@model AppData.Model.KieuDang
@{
    ViewData["Title"] = "EditKieuDang";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<h1>Update</h1>
<h4>KieuDang</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" method="post">
            @Html.AntiForgeryToken()
            @Html.HiddenFor(m => m.IdKieuDang)
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TenKieuDang" class="control-label"></label>
                <input asp-for="TenKieuDang" class="form-control" />
                <span asp-validation-for="TenKieuDang" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NguoiTao" class="control-label"></label>
                <input asp-for="NguoiTao" class="form-control" />
                <span asp-validation-for="NguoiTao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="KichHoat" class="control-label"></label>
                <input asp-for="KichHoat" class="form-control" />
                <span asp-validation-for="KichHoat" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Getall">Cancel</a>
</div>




